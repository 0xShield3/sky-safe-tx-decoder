{
  "name": "sky-safe-tx-decoder",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript-based Safe multisig transaction hash calculator and decoder with custom protocol support",
  "keywords": [
    "safe",
    "gnosis-safe",
    "multisig",
    "eip-712",
    "transaction-hash"
  ],
  "author": "Security Alliance",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/securityalliance/sky-safe-tx-decoder"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "dev:cli": "pnpm --filter @shield3/sky-safe-cli dev"
  },
  "devDependencies": {
    "@types/node": "20.19.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vitest": "2.0.0"
  },
  "pnpm": {
    "overrides": {
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "esbuild@<=0.24.2": ">=0.25.0",
      "vitest@>=1.0.0 <1.6.1": ">=1.6.1",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "next@>=16.0.0-canary.0 <16.0.7": ">=16.0.7",
      "next@>=16.0.0-beta.0 <16.0.9": ">=16.0.9",
      "react-router@>=7.0.0 <=7.11.0": ">=7.12.0",
      "react-router@>=7.0.0 <7.12.0": ">=7.12.0",
      "next@>=15.6.0-canary.0 <16.1.5": ">=16.1.5",
      "next@>=16.0.0-beta.0 <16.0.11": ">=16.0.11",
      "next@>=16.0.0-beta.0 <16.1.5": ">=16.1.5"
    }
  }
}
