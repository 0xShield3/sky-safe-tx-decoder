  "dependencies": {
    "@shield3/sky-safe-core": "workspace:*",
    "@types/inquirer": "9.0.9",
    "chalk": "5.3.0",
    "commander": "11.1.0",
    "inquirer": "12.11.1",
    "ora": "8.0.1",
    "viem": "2.7.0"
  },
  "devDependencies": {
