{
  "name": "@shield3/sky-safe-cli",
  "version": "0.1.0",
  "description": "CLI for Safe transaction hash calculation and verification",
  "type": "module",
  "bin": {
    "sky-safe": "./dist/index.js"
  },
  "files": [
    "dist"
